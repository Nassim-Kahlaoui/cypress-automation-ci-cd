{
  "name": "cypress-ecomerce",
  "version": "1.0.0",
  "description": "Projet de tests e-commerce avec Cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress run && npm run merge-report && node write-results.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:api": "npx cypress run --spec \"testapi/*.spec.js\"",
    "merge-report": "mochawesome-merge cypress/reports/mochawesome/mochawesome_*.json > cypress/reports/mochawesome/mochawesome.json",
    "generate-html-report": "marge cypress/reports/mochawesome/mochawesome.json -o cypress/reports/mochawesome/html-report"
  },
  "keywords": [
    "cypress",
    "e2e",
    "test",
    "ecommerce"
  ],
  "author": "Nassim",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0"
  }
}
